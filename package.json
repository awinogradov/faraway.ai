{
  "name": "faraway.ai",
  "description": "Faraway AI",
  "version": "0.0.1",
  "license": "MIT",
  "main": "src/index.ts",
  "author": "Anton Vinodradov <winogradovaa@gmail.com>",
  "scripts": {
    "seed": "ts-node scripts/seed.ts",
    "dev": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start": "NODE_ENV=production node build/server.js",
    "prestage": "npm run build && docker build -t farawayai .",
    "stage": "docker run -p 5000:5000 --env-file .env.stage -e PORT=5000 farawayai",
    "registry": "npm run build && heroku container:push web -a farawayai",
    "production": "heroku container:release web -a farawayai"
  },
  "dependencies": {
    "await-to-js": "2.1.1",
    "bcrypt": "3.0.6",
    "body-parser": "1.19.0",
    "class-validator": "0.9.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "express": "4.16.4",
    "firebase-tools": "6.10.0",
    "graphql": "14.2.1",
    "graphql-subscriptions": "1.1.0",
    "graphql-tag": "2.10.1",
    "graphql-tools": "4.0.4",
    "graphql-yoga": "1.17.4",
    "helmet": "3.16.0",
    "jsonwebtoken": "8.5.1",
    "mongodb": "3.2.3",
    "morgan": "1.9.1",
    "parse-error": "0.2.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.0",
    "really-typed-compose": "1.0.1",
    "reflect-metadata": "0.1.13",
    "type-graphql": "0.17.3",
    "typedi": "0.8.0",
    "typeorm": "0.2.16",
    "typeorm-typedi-extensions": "0.2.3",
    "unfetch": "4.1.0",
    "universal-cookie": "4.0.0",
    "uuid": "3.3.2",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.25",
    "@types/bcrypt": "3.0.0",
    "@types/body-parser": "1.17.0",
    "@types/chai": "4.1.7",
    "@types/chai-subset": "1.3.2",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "1.4.1",
    "@types/cors": "2.8.5",
    "@types/express": "4.16.1",
    "@types/faker": "4.1.5",
    "@types/helmet": "0.0.43",
    "@types/jest": "24.0.12",
    "@types/jsonwebtoken": "8.3.2",
    "@types/morgan": "1.7.35",
    "@types/node": "12.0.0",
    "@types/node-fetch": "2.3.3",
    "@types/passport": "1.0.0",
    "@types/passport-jwt": "3.0.1",
    "@types/passport-local": "1.0.33",
    "@types/react": "16.8.16",
    "@types/react-dom": "16.0.5",
    "@types/react-router-dom": "4.3.3",
    "@types/supertest": "2.0.7",
    "@types/uuid": "3.4.4",
    "@types/webpack-env": "1.13.9",
    "@types/ws": "6.0.1",
    "chai": "4.2.0",
    "chai-subset": "1.6.0",
    "faker": "4.1.0",
    "json-to-graphql-query": "1.9.0",
    "mongo-unit": "1.4.4",
    "razzle": "3.0.0",
    "razzle-plugin-typescript": "3.0.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "tslint-react": "4.0.0",
    "typescript": "3.4.5"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
